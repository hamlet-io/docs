              "additionalProperties": false,
              "type": "object",
              "properties": {
                "ProviderIds": {
                  "contains": {
                    "type": [
                      "string"
                    ]
                  },
                  "description": "The provider Ids of provider accounts to share with",
                  "type": "array"
                }
              }
            }
