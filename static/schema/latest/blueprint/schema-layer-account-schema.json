{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "definitions": {
    "account": {
      "patternProperties": {
        "^[A-Za-z_][A-Za-z0-9_]*$": {
          "additionalProperties": false,
          "properties": {
            "ProviderId": {
              "type": "string"
            },
            "S3": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "Versioning": {
                  "additionalProperties": false,
                  "type": "object",
                  "properties": {
                    "Enabled": {
                      "default": false,
                      "type": "boolean"
                    }
                  }
                },
                "Encryption": {
                  "additionalProperties": false,
                  "type": "object",
                  "properties": {
                    "EncryptionSource": {
                      "default": "EncryptionService",
                      "type": "string",
                      "enum": [
                        "EncryptionService",
                        "aws:kms",
                        "localservice",
                        "aes256"
                      ]
                    },
                    "Enabled": {
                      "default": false,
                      "type": "boolean"
                    },
                    "Encryption": {
                      "default": false,
                      "type": "boolean"
                    }
                  }
                }
              }
            },
            "aws:ecsAccountSettings": {
              "default": {
                "containerInstanceLongArnFormat": true,
                "serviceLongArnFormat": true,
                "taskLongArnFormat": true,
                "awsvpcTrunking": true
              },
              "type": "object"
            },
            "Description": {
              "type": "string"
            },
            "Modules": {
              "$ref": "https://docs.hamlet.io/schema/latest/blueprint/schema-attributeset-module-schema.json"
            },
            "Access": {
              "patternProperties": {
                "^[A-Za-z_][A-Za-z0-9_]*$": {
                  "additionalProperties": false,
                  "properties": {
                    "AWSId": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "AWSId"
                  ]
                }
              },
              "type": "object"
            },
            "Encryption": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "Alias": {
                  "additionalProperties": false,
                  "type": "object",
                  "properties": {
                    "IncludeSeed": {
                      "default": false,
                      "type": "boolean"
                    }
                  }
                }
              }
            },
            "Name": {
              "type": "string"
            },
            "Profiles": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "Policy": {
                  "contains": {
                    "type": "string"
                  },
                  "type": "array"
                },
                "Deployment": {
                  "contains": {
                    "type": "string"
                  },
                  "type": "array"
                }
              }
            },
            "Console": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "Encryption": {
                  "additionalProperties": false,
                  "type": "object",
                  "properties": {
                    "DedicatedKey": {
                      "default": false,
                      "type": "boolean"
                    }
                  }
                },
                "LoggingDestinations": {
                  "contains": {
                    "type": "string"
                  },
                  "default": [
                    "s3"
                  ],
                  "type": "array",
                  "enum": [
                    "cloudwatch",
                    "s3"
                  ]
                }
              }
            },
            "Operations": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "Expiration": {
                  "type": "number"
                }
              }
            },
            "CostCentre": {
              "type": "string"
            },
            "Plugins": {
              "$ref": "https://docs.hamlet.io/schema/latest/blueprint/schema-attributeset-plugin-schema.json"
            },
            "DeploymentProfiles": {
              "type": "object"
            },
            "PolicyProfiles": {
              "type": "object"
            },
            "Title": {
              "type": "string"
            },
            "Registry": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "Registries": {
                  "description": "Defines the registries available for image hosting",
                  "additionalProperties": false,
                  "type": "object",
                  "properties": {
                    "pipeline": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "objectstore",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "objectstore"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    },
                    "contentnode": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "objectstore",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "objectstore"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    },
                    "lambda": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "objectstore",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "objectstore"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    },
                    "spa": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "objectstore",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "objectstore"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    },
                    "openapi": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "objectstore",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "objectstore"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    },
                    "rdssnapshot": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "snapshotstore",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "snapshotstore"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    },
                    "scripts": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "objectstore",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "objectstore"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    },
                    "dataset": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "objectstore",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "objectstore"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    },
                    "swagger": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "objectstore",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "objectstore"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    },
                    "docker": {
                      "additionalProperties": false,
                      "type": "object",
                      "properties": {
                        "Storage": {
                          "default": "providerregistry",
                          "description": "How the images are stored",
                          "type": "object",
                          "enum": [
                            "providerregistry"
                          ]
                        },
                        "Enabled": {
                          "default": true,
                          "type": "object"
                        }
                      }
                    }
                  }
                },
                "ReplicaRegions": {
                  "contains": {
                    "type": "string"
                  },
                  "description": "A list of regions to replicate registries to",
                  "additionalProperties": false,
                  "type": "array",
                  "properties": {
                    "AWSAccounts": {
                      "contains": {
                        "type": "string"
                      },
                      "type": "array"
                    }
                  }
                }
              }
            },
            "Provider": {
              "default": "aws",
              "description": "Define which provider this account applies to",
              "type": "string"
            },
            "Audit": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "Expiration": {
                  "anyOf": [
                    {
                      "type": "number"
                    },
                    {
                      "type": "string"
                    }
                  ]
                },
                "Offline": {
                  "anyOf": [
                    {
                      "type": "number"
                    },
                    {
                      "type": "string"
                    }
                  ]
                }
              }
            },
            "Volume": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "Encryption": {
                  "additionalProperties": false,
                  "type": "object",
                  "properties": {
                    "Enabled": {
                      "default": false,
                      "type": "boolean"
                    }
                  }
                }
              }
            },
            "Seed": {
              "type": "string"
            },
            "Region": {
              "type": "string"
            },
            "aws:SES": {
              "description": "AWS SES Account configuration",
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "RuleSet": {
                  "description": "Ruleset details. Only one active per account",
                  "additionalProperties": false,
                  "type": "object",
                  "properties": {
                    "Name": {
                      "default": "account-default",
                      "description": "Name of the ruleset",
                      "type": "string"
                    }
                  }
                },
                "IPAddressGroups": {
                  "contains": {
                    "type": "string"
                  },
                  "type": "array"
                }
              }
            },
            "Id": {
              "type": "string"
            },
            "Domain": {
              "type": "string"
            }
          },
          "required": [
            "Seed"
          ]
        }
      },
      "type": "object"
    }
  },
  "$id": "https://docs.hamlet.io/schema/latest/blueprint/schema-layer-account-schema.json"
}
